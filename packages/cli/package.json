{
  "name": "@readyapi/cli",
  "description": "A command-line interface to work with OpenAPI files",
  "license": "MIT",
  "author": "Khulnasoft (https://github.com/khulnasoft)",
  "homepage": "https://github.com/khulnasoft/readyapi.js",
  "bugs": "https://github.com/khulnasoft/readyapi.js/issues/new/choose",
  "repository": {
    "type": "git",
    "url": "https://github.com/khulnasoft/readyapi.js.git",
    "directory": "packages/cli"
  },
  "keywords": [
    "readyapi",
    "openapi",
    "swagger",
    "cli"
  ],
  "version": "0.2.75",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "@readyapi/cli": "pnpm vite-node src/index.ts",
    "build": "rm -Rf dist/ && tsc && rollup -c",
    "cli:link": "pnpm run build && npm unlink -g && npm link",
    "types:check": "tsc --noEmit --skipLibCheck"
  },
  "type": "module",
  "files": [
    "./dist"
  ],
  "bin": {
    "readyapi": "./dist/index.js"
  },
  "dependencies": {
    "@hono/node-server": "^1.11.1",
    "@parcel/watcher": "^2.4.1",
    "@readyapi/api-reference": "workspace:*",
    "@readyapi/mock-server": "workspace:*",
    "@readyapi/oas-utils": "workspace:*",
    "@readyapi/openapi-parser": "^0.3.3",
    "commander": "^12.0.0",
    "glob": "^10.3.15",
    "hono": "^4.3.6",
    "kleur": "^4.1.5",
    "openapi-types": "^12.1.3",
    "prettyjson": "^1.2.5",
    "prompts": "^2.4.2"
  },
  "devDependencies": {
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/node": "^20.12.12",
    "execa": "^8.0.1",
    "rollup": "^4.17.2",
    "rollup-plugin-delete": "^2.0.0",
    "strip-ansi": "^7.1.0",
    "tslib": "^2.6.2",
    "typescript": "^5.4.5",
    "vite-node": "^1.6.0"
  }
}
