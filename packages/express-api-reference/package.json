      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "module": "dist/index.js",
  "dependencies": {
    "@readyapi/api-reference": "workspace:*",
    "express": "^4.19.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "tsup": "^7.2.0",
    "vite": "^5.2.10",
    "vitest": "^1.5.0"
  },
  "peerDependencies": {
    "express": "^4.0.0"
  }
