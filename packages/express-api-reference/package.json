  "author": "KhulnaSoft (https://github.com/khulnasoft)",
  "homepage": "https://github.com/khulnasoft/readyapi",
  "bugs": "https://github.com/khulnasoft/readyapi/issues/new/choose",
  "repository": {
    "type": "git",
    "url": "https://github.com/khulnasoft/readyapi.git",
    "directory": "packages/use-tooltip"
  },
  "version": "0.4.35",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "build": "tsup ./src/index.ts --format esm,cjs --dts",
    "test": "vitest run"
  },
  "type": "module",
  "main": "dist/index.cjs",
  "types": "dist/index.d.cts",
